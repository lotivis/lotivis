{
  "name": "lotivis",
  "version": "1.0.94",
  "description": "Location and time data visualization",
  "homepage": "https://github.com/lukasdanckwerth/lotivis#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukasdanckwerth/lotivis.git"
  },
  "keywords": [
    "location",
    "time",
    "data",
    "visualization"
  ],
  "license": "MIT",
  "author": "Lukas Danckwerth",
  "type-bu": "module",
  "module": "index",
  "jsdelivr": [
    "dist/lotivis.css",
    "dist/lotivis.min.css",
    "dist/lotivis.js",
    "dist/lotivis.js.map"
  ],
  "unpkg": "dist/lotivis.js",
  "main": "dist/lotivis.js",
  "file": "dist/lotivis.js",
  "files": [
    "dist/lotivis.css",
    "dist/lotivis.min.css",
    "dist/lotivis.js",
    "dist/lotivis.js.map"
  ],
  "scripts": {
    "develop": "rollup -c -w",
    "serve": "http-server",
    "build": "rollup -c",
    "scss": "sass src/index.scss > dist/lotivis.css && sass --style compressed src/index.scss > dist/lotivis.min.css",
    "test": "mocha 'test/**/test.*.js'",
    "copy:to:frcv": "cp dist/lotivis.js ../frc-visualization/public/js/lib && cp dist/lotivis.js.map ../frc-visualization/public/js/lib && cp dist/lotivis.css ../frc-visualization/public/css"
  },
  "bugs": {
    "url": "https://github.com/lukasdanckwerth/lotivis/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "lint": "^1.1.2",
    "mocha": "^8.3.0",
    "rollup": "^2.39.0",
    "rollup-plugin-execute": "^1.1.1",
    "sass": "^1.32.8"
  },
  "dependencies": {
    "d3": "7.1.0",
    "topojson-client": "^3.1.0",
    "topojson-server": "^3.0.1",
    "topojson-simplify": "^3.0.3"
  }
}
