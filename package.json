{
    "name": "lotivis",
    "version": "1.0.102",
    "description": "Location and time data visualization",
    "license": "MIT",
    "author": "Lukas Danckwerth",
    "type": "module",
    "keywords": [
        "location",
        "time",
        "data",
        "visualization"
    ],
    "homepage": "https://github.com/lukasdanckwerth/lotivis#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lukasdanckwerth/lotivis.git"
    },
    "bugs": {
        "url": "https://github.com/lukasdanckwerth/lotivis/issues"
    },
    "main": "dist/lotivis.js",
    "module": "dist/lotivis.esm.js",
    "jsdelivr": [
        "dist/lotivis.css",
        "dist/lotivis.min.css",
        "dist/lotivis.js",
        "dist/lotivis.js.map",
        "dist/lotivis.esm.js",
        "dist/lotivis.esm.js.map"
    ],
    "files": [
        "dist/**/*",
        "src/**/*"
    ],
    "scripts": {
        "build": "rollup --config",
        "build:watch": "rollup --config --watch",
        "build:watch:copy": "rollup --config rollup.config.hooks.js --watch",
        "serve": "http-server",
        "scss": "sass src/index.scss > dist/lotivis.css && sass --style compressed src/index.scss > dist/lotivis.min.css",
        "test": "npx mocha --require esm --reporter spec 'test/test.*.js'",
        "copy": "npm run scss && cp dist/lotivis.js ../frc-visualization/public/js/lib && cp dist/lotivis.js.map ../frc-visualization/public/js/lib && cp dist/lotivis.css ../frc-visualization/public/css"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^11.1.1",
        "esm": "^3.2.25",
        "mocha": "^9.2.0",
        "rollup": "^2.39.0",
        "rollup-plugin-execute": "^1.1.1",
        "sass": "^1.32.8"
    },
    "dependencies": {
        "d3": "^7.3.0",
        "topojson-client": "^3.1.0",
        "topojson-server": "^3.0.1",
        "topojson-simplify": "^3.0.3"
    }
}
