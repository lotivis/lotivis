{
  "name": "lotivis",
  "homepage": "https://github.com/lukasdanckwerth/lotivis#readme",
  "description": "Location and time data visualization",
  "license": "MIT",
  "version": "1.0.22",
  "jsdelivr": [
    "dist/lotivis.css",
    "dist/lotivis.js",
    "dist/lotivis.js.map"
  ],
  "unpkg": "dist/lotivis.js",
  "main": "dist/lotivis.js",
  "file": "dist/lotivis.js",
  "files": [
    "dist/lotivis.css",
    "dist/lotivis.js",
    "dist/lotivis.js.map"
  ],
  "scripts": {
    "develop": "rollup -c -w",
    "build:js": "rollup -c",
    "build:css:sass": "sass src/main.scss > dist/lotivis.css",
    "dev-alt": "karma start --auto-watch --no-single-run --browsers safari --grep",
    "lint-js": "eslint src/js/**/*.js test/*.js",
    "test": "mocha",
    "test-alt": "npm run lint-js && cross-env NODE_ENV=test karma start --auto-watch --single-run --coverage --grep",
    "example": "npm run build:js && npm run build:css:sass && node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukasdanckwerth/lotivis.git"
  },
  "keywords": [
    "location",
    "time",
    "data",
    "visualization"
  ],
  "author": "Lukas Danckwerth",
  "bugs": {
    "url": "https://github.com/lukasdanckwerth/lotivis/issues"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "cross-env": "^7.0.3",
    "d3": "^6.5.0",
    "eslint": "^7.19.0",
    "express": "^4.17.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.0",
    "karma-requirejs": "^1.1.0",
    "karma-safari-launcher": "^1.0.0",
    "lint": "^0.7.0",
    "mocha": "^8.3.0",
    "rollup": "^2.39.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.8"
  }
}
